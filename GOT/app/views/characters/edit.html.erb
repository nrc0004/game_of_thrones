<h2>Edit Character</h2>

<%= form_for [@house, @character] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.submit %>
<% end %>

<%= link_to "Delete", @character, method: :delete, class: "delete" %>
