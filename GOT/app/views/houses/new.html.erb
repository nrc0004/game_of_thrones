<h2>Create a new house</h2>

<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :sigil_url %>
  <%= f.text_field :sigil_url %>

  <%= f.label :words %>
  <%= f.text_field :words %>

  <%= f.label :seat %>
  <%= f.text_field :seat %>

  <%= f.submit %>
<% end %>
