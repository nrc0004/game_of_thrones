<h2>Edit House</h2>

<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :words %>
  <%= f.text_field :words %>

  <%= f.label :sigil_url %>
  <%= f.text_field :sigil_url %>

  <%= f.label :seat %>
  <%= f.text_field :seat %>

  <%= f.submit %>
<% end %>

<%= link_to "Delete", @house, method: :delete, class: "delete" %>
